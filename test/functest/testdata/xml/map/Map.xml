<MapConf Open="true">
    <PreEntry Name="1" Cost="1,2,3" />
    <PreEntry Name="2" Cost="4,5,6" />
    <Entry Id="1" Value="1" IncellMap="1:first,2:second" />
    <Entry Id="2" Value="2" IncellMap=""/>
    <Entry Id="3" IncellMap="3:third" />
    <Incell Map="Tom:1,Jerry:2" />
    <IncellMap>1:1,2:10,3:100</IncellMap>
    <KeyEnumMap>Apple:1,Banana:2</KeyEnumMap>
    <ValueEnumMap>1:Banana,2:Orange</ValueEnumMap>
    <KvEnumMap>Apple:Banana,Banana:Orange</KvEnumMap>

    <Meal Id="breakfast" Time="15m">
        <Meat Name="Egg" Weight="100g" />
        <VegetableInfo>
            <Vegetable Name="Cauliflower" Num="2" />
            <Vegetable Name="Cucumber" Num="1" />
        </VegetableInfo>
        <FruitInfo>
            <Fruit Name="Apple" Desc="sour" />
        </FruitInfo>
    </Meal>
    <Meal Id="lunch" Time="20m">
        <Meat Name="Chicken" Weight="150g" />
        <Meat Name="Fish" Weight="100g" />
        <VegetableInfo>
            <Vegetable Name="Spinach" Num="1" />
        </VegetableInfo>
        <FruitInfo>
            <Fruit Name="Mongo" Desc="sweet" />
        </FruitInfo>
    </Meal>
    <Meal Id="dinner">
        <Meat Name="Beef" Weight="200g" />
        <VegetableInfo />
        <FruitInfo>
            <Fruit Name="Banana" />
        </FruitInfo>
    </Meal>
</MapConf>
